{
  "build": {
    "arduino":{
      "ldscript": "esp32s2_out.ld"
    },
    "core": "esp32",
    "extra_flags": [
      "-DARDUINO_IOTPOSTBOX_V1",
      "-DBOARD_HAS_PSRAM",
      "-DARDUINO_USB_CDC_ON_BOOT=1"
    ],
    "f_cpu": "240000000L",
    "f_flash": "80000000L",
    "flash_mode": "dio",
    "hwids": [
      [
        "0x303A",
        "0x8102"
      ],
      [
        "0x303A",
        "0x8103"
      ],
      [
        "0x303A",
        "0x8104"
      ]
    ],
    "mcu": "esp32s2",
    "variant": "iotpostbox_v1"
  },
  "connectivity": [
    "wifi"
  ],
  "debug": {
    "openocd_target": "esp32s2.cfg"
  },
  "frameworks": [
    "arduino",
    "espidf"
  ],
  "name": "IoT-PostBox v1 ESP32-S2 Development Board",
  "upload": {
    "arduino": {
      "flash_extra_images": [
        [
          "0x410000",
          "variants/iotpostbox_v1/tinyuf2.bin"
        ]
      ]
    },
    "flash_size": "16MB",
    "maximum_ram_size": 327680,
    "maximum_size": 16777216,
    "use_1200bps_touch": true,
    "wait_for_upload_port": true,
    "require_upload_port": true,
    "speed": 921600
  },
  "url": "https://github.com/paclema/iot-postbox",
  "vendor": "paclema"
}
